{
  "name": "churchconnect-event-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "lucide-react": "^0.536.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
"scripts": {
  "start": "react-scripts start",
  "build": "react-scripts build",
  "test": "react-scripts test",
  "eject": "react-scripts eject",
  "clean": "rmdir /s /q node_modules 2>nul & del package-lock.json 2>nul",
  "clean:cache": "npm cache clean --force",
  "reinstall": "npm run clean && npm run clean:cache && npm install",
  "fix:install": "npm run clean && npm install react-scripts@5.0.1 --force && npm install",
  "fix:audit": "npm audit fix",
  "fresh-start": "npm run clean && npm run clean:cache && npm install react@18.2.0 react-dom@18.2.0 --save && npm install react-scripts@5.0.1 --force && npm install"
},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
